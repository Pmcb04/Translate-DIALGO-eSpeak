    5 programa: salto_opc bloquePersonajes bloqueDefiniciones secEscena

    6 $@1: %empty

    7 bloquePersonajes: PERSONAJES $@1 salto secBloquePersonajes

    8 secBloquePersonajes: asignacionPersonaje
    9                    | secBloquePersonajes asignacionPersonaje

   10 asignacionPersonaje: ID_NOMBRE '=' '<' IDIOMA ',' VOZ '>' salto
   11                    | error salto

   12 bloqueDefiniciones: %empty

   13 $@2: %empty

   14 bloqueDefiniciones: DEFINICIONES $@2 salto secBloqueDefiniciones

   15 secBloqueDefiniciones: identificador
   16                      | secBloqueDefiniciones identificador
   17                      | secBloqueDefiniciones error salto

   18 identificador: identificadorCadena
   19              | identificadorGeneral

   20 identificadorGeneral: ID_GENERAL '=' expr_arit salto
   21                     | ID_GENERAL '=' expr_log salto

   22 identificadorCadena: ID_CADENA '=' expr_cadena salto

   23 secEscena: escena
   24          | secEscena escena

   25 $@3: %empty

   26 escena: ESCENA expr_arit ':' salto $@3 secBloqueEscena FINESCENA salto

   27 secBloqueEscena: bloqueEscena
   28                | secBloqueEscena bloqueEscena

   29 bloqueEscena: lineaEscena
   30             | identificador

   31 $@4: %empty

   32 bloqueEscena: $@4 bucle
   33             | condicional
   34             | error salto

   35 lineaEscena: ID_NOMBRE ':' expr_cadena salto
   36            | ID_NOMBRE '[' ']' ':' expr_cadena salto
   37            | ID_NOMBRE '[' entonacion ']' ':' expr_cadena salto
   38            | MENSAJE expr_cadena salto
   39            | PAUSA expr_arit salto

   40 $@5: %empty

   41 bucle: REPETIR expr_arit '{' salto $@5 secBloqueEscena '}' salto

   42 condicional: parteSi parteSiNo

   43 $@6: %empty

   44 parteSi: SI '(' expr_log ')' salto_opc '{' salto $@6 secBloqueEscena '}' salto

   45 parteSiNo: %empty

   46 $@7: %empty

   47 parteSiNo: SI_NO salto_opc '{' salto $@7 secBloqueEscena '}' salto

   48 entonacion: TONO
   49           | entonacion ',' TONO

   50 expr_cadena: CADENA
   51            | ID_CADENA
   52            | expr_arit
   53            | expr_cadena CONCATENACION expr_cadena

   54 expr_log: TRUE
   55         | FALSE
   56         | expr_log AND expr_log
   57         | expr_log OR expr_log
   58         | NOT expr_log
   59         | '(' expr_log ')'
   60         | expr_arit '>' expr_arit
   61         | expr_arit MAYORIGUAL expr_arit
   62         | expr_arit '<' expr_arit
   63         | expr_arit MENORIGUAL expr_arit
   64         | expr_arit IGUAL2 expr_arit
   65         | expr_arit DISTINTO expr_arit
   66         | expr_log IGUAL2 expr_log
   67         | expr_log DISTINTO expr_log

   68 expr_arit: NUMERO
   69          | REAL
   70          | ID_GENERAL
   71          | expr_arit '+' expr_arit
   72          | expr_arit '-' expr_arit
   73          | expr_arit '*' expr_arit
