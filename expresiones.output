Gramática

    0 $accept: programa $end

    1 salto: '\n'
    2      | salto '\n'

    3 salto_opc: %empty
    4          | salto_opc '\n'

    5 programa: salto_opc bloquePersonajes bloqueDefiniciones secEscena

    6 $@1: %empty

    7 bloquePersonajes: PERSONAJES $@1 salto secBloquePersonajes

    8 secBloquePersonajes: asignacionPersonaje
    9                    | secBloquePersonajes asignacionPersonaje

   10 asignacionPersonaje: ID_NOMBRE '=' '<' IDIOMA ',' VOZ '>' salto
   11                    | error salto

   12 bloqueDefiniciones: %empty

   13 $@2: %empty

   14 bloqueDefiniciones: DEFINICIONES $@2 salto secBloqueDefiniciones

   15 secBloqueDefiniciones: identificador
   16                      | secBloqueDefiniciones identificador

   17 identificador: identificadorCadena
   18              | identificadorGeneral
   19              | error salto

   20 identificadorGeneral: ID_GENERAL '=' expr_arit salto
   21                     | ID_GENERAL '=' expr_log salto

   22 identificadorCadena: ID_CADENA '=' expr_cadena salto

   23 secEscena: escena
   24          | secEscena escena

   25 $@3: %empty

   26 escena: ESCENA expr_arit ':' salto $@3 secBloqueEscena FINESCENA salto

   27 secBloqueEscena: bloqueEscena
   28                | secBloqueEscena bloqueEscena

   29 bloqueEscena: ID_NOMBRE ':' expr_cadena salto
   30             | ID_NOMBRE '[' ']' ':' expr_cadena salto
   31             | ID_NOMBRE '[' entonacion ']' ':' expr_cadena salto
   32             | MENSAJE expr_cadena salto
   33             | PAUSA expr_arit salto
   34             | identificadorCadena
   35             | identificadorGeneral
   36             | condicional
   37             | bucle
   38             | error salto

   39 $@4: %empty

   40 bucle: REPETIR expr_arit '{' salto $@4 secBloqueEscena '}' salto

   41 condicional: parteSi parteSiNo

   42 $@5: %empty

   43 parteSi: SI '(' expr_log ')' salto_opc '{' salto $@5 secBloqueEscena '}' salto

   44 parteSiNo: %empty

   45 $@6: %empty

   46 parteSiNo: SI_NO salto_opc '{' salto $@6 secBloqueEscena '}' salto

   47 entonacion: TONO
   48           | entonacion ',' TONO

   49 expr_cadena: CADENA
   50            | ID_CADENA
   51            | expr_arit
   52            | expr_cadena CONCATENACION expr_cadena

   53 expr_log: TRUE
   54         | FALSE
   55         | expr_log AND expr_log
   56         | expr_log OR expr_log
   57         | NOT expr_log
   58         | '(' expr_log ')'
   59         | expr_arit '>' expr_arit
   60         | expr_arit MAYORIGUAL expr_arit
   61         | expr_arit '<' expr_arit
   62         | expr_arit MENORIGUAL expr_arit
   63         | expr_arit IGUAL2 expr_arit
   64         | expr_arit DISTINTO expr_arit
   65         | expr_log IGUAL2 expr_log
   66         | expr_log DISTINTO expr_log

   67 expr_arit: NUMERO
