   17 identificador: identificadorCadena
   18              | identificadorGeneral

   19 $@4: %empty

   20 identificadorGeneral: ID_GENERAL '=' expr_arit $@4 salto

   21 $@5: %empty

   22 identificadorGeneral: ID_GENERAL '=' expr_log $@5 salto

   23 $@6: %empty

   24 identificadorCadena: ID_CADENA '=' expr_cadena $@6 salto

   25 secEscena: escena
   26          | secEscena escena

   27 $@7: %empty

   28 $@8: %empty

   29 escena: ESCENA expr_arit ':' $@7 salto secBloqueEscena FINESCENA $@8 salto

   30 secBloqueEscena: bloqueEscena
