Gramática

    0 $accept: programa $end

    1 salto: '\n'
    2      | salto '\n'

    3 salto_opc: %empty
    4          | salto_opc '\n'

    5 programa: salto_opc bloquePersonajes bloqueDefiniciones secEscena

    6 $@1: %empty

    7 bloquePersonajes: PERSONAJES $@1 salto secBloquePersonajes

    8 secBloquePersonajes: asignacionPersonaje
    9                    | secBloquePersonajes asignacionPersonaje

   10 $@2: %empty

   11 asignacionPersonaje: ID_NOMBRE '=' '<' idioma ',' voz '>' $@2 salto

   12 bloqueDefiniciones: %empty

   13 $@3: %empty

   14 bloqueDefiniciones: DEFINICIONES $@3 salto secBloqueDefiniciones

   15 secBloqueDefiniciones: identificador
   16                      | secBloqueDefiniciones identificador

   17 identificador: identificadorCadena
   18              | identificadorGeneral

   19 $@4: %empty

   20 identificadorGeneral: ID_GENERAL '=' expr_arit $@4 salto

   21 $@5: %empty

   22 identificadorGeneral: ID_GENERAL '=' expr_log $@5 salto

   23 $@6: %empty

   24 identificadorCadena: ID_CADENA '=' expr_cadena $@6 salto

   25 secEscena: escena
   26          | secEscena escena

   27 $@7: %empty

   28 $@8: %empty

   29 escena: ESCENA expr_arit ':' $@7 salto secBloqueEscena FINESCENA $@8 salto

   30 secBloqueEscena: bloqueEscena
   31                | secBloqueEscena bloqueEscena

   32 $@9: %empty

   33 bloqueEscena: ID_NOMBRE ':' expr_cadena $@9 salto

   34 $@10: %empty

   35 bloqueEscena: ID_NOMBRE '[' ']' ':' expr_cadena $@10 salto

   36 $@11: %empty

   37 bloqueEscena: ID_NOMBRE '[' entonacion ']' ':' expr_cadena $@11 salto

   38 $@12: %empty

   39 bloqueEscena: MENSAJE expr_cadena $@12 salto

   40 $@13: %empty

   41 bloqueEscena: PAUSA expr_arit $@13 salto
   42             | identificador
   43             | condicional
   44             | bucle

   45 $@14: %empty

   46 bucle: REPETIR expr_arit '{' salto $@14 secBloqueEscena '}' salto

   47 condicional: parteSi parteSiNo

   48 $@15: %empty

   49 parteSi: SI '(' expr_log ')' salto_opc '{' salto $@15 secBloqueEscena '}' salto

   50 parteSiNo: %empty

   51 $@16: %empty

   52 parteSiNo: SI_NO '{' salto $@16 secBloqueEscena '}' salto

   53 entonacion: tono
   54           | entonacion ',' tono

   55 tono: DESPACIO
   56     | DEPRISA
   57     | GRITANDO
   58     | VOZ_BAJA

   59 idioma: EN
   60       | EN_US
   61       | ES
   62       | ES_LA
   63       | PT
   64       | IT
   65       | FR

   66 voz: MASCULINO
   67    | FEMENINO

   68 expr_cadena: CADENA
   69            | ID_CADENA
   70            | expr_arit
   71            | expr_cadena CONCATENACION expr_cadena

   72 expr_log: TRUE
   73         | FALSE
   74         | expr_log AND expr_log
   75         | expr_log OR expr_log
   76         | NOT expr_log
   77         | '(' expr_log ')'
   78         | expr_arit '>' expr_arit
   79         | expr_arit MAYORIGUAL expr_arit
   80         | expr_arit '<' expr_arit
   81         | expr_arit MENORIGUAL expr_arit
   82         | expr_arit IGUAL2 expr_arit
   83         | expr_arit DISTINTO expr_arit
   84         | expr_log IGUAL2 expr_log
   85         | expr_log DISTINTO expr_log

   86 expr_arit: NUMERO
   87          | REAL
   88          | ID_GENERAL
   89          | expr_arit '+' expr_arit
   90          | expr_arit '-' expr_arit
   91          | expr_arit '*' expr_arit
