   38 $@12: %empty

   39 bloqueEscena: MENSAJE expr_cadena $@12 salto

   40 $@13: %empty

   41 bloqueEscena: PAUSA expr_arit $@13 salto
   42             | identificador
   43             | condicional

   44 condicional: parteSi parteSiNo

   45 $@14: %empty

   46 parteSi: SI '(' expr_log ')' salto_opc '{' salto $@14 secBloqueEscena '}' salto

   47 parteSiNo: %empty

   48 $@15: %empty

   49 parteSiNo: SI_NO '{' salto $@15 secBloqueEscena '}' salto

   50 entonacion: tono
